<h1>Listing as</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @as.each do |a| %>
  <tr>
    <td><%= a.name %></td>
    <td><%= link_to 'Show', a %></td>
    <td><%= link_to 'Edit', edit_a_path(a) %></td>
    <td><%= link_to 'Destroy', a, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

